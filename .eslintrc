{
    "env": {
        "browser": true,
        "jest": true
    },
    "globals": {
        "global": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2015,
        "lib": [
            "DOM",
            "ES2015"
        ]
    },
    "plugins": [
        "@typescript-eslint/eslint-plugin"
    ],
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "indent": [
            "error",
            4
        ],
        "no-extra-semi": "error",
        "@typescript-eslint/no-explicit-any": "off",
        "react/prop-types": "off",
        "quotes": [
            "error",
            "double"
        ],
        "no-console": "error",
        "@typescript-eslint/no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars-experimental": "error",
        "no-unused-vars": "off"
    }
}