{
  "private": true,
  "name": "history-router-elements",
  "version": "0.1.1",
  "description": "minimal declarative custom element pushstate routing for browser apps",
  "main": "dist/history-router.js",
  "browser": "dist/history-router.umd.js",
  "module": "dist/history-router.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/nnnnathann/history-router-elements.git"
  },
  "author": "Nathan Bleigh <nathan.bleigh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rollup -c rollup.config.js -w",
    "build": "run-p build:*",
    "build:bundle": "NODE_ENV=production rollup -c rollup.config.js",
    "build:js": "babel -d lib -x .ts src",
    "test": "run-s build test:jest",
    "test:jest": "jest",
    "pkg": "run-s build test pkg:make pkg:publish",
    "pkg:make": "babel-node -x .ts scripts/build-package.ts",
    "pkg:publish": "babel-node -x .ts scripts/publish.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "core-js": "^3.9.1",
    "eslint": "^7.22.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.9.2",
    "pushstate-server": "^3.1.0",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.42.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.3",
    "webpack": "^5.27.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "history": "^5.0.0"
  }
}